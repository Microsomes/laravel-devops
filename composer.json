{
    "name": "microsomes/laravel-devops",
    "description": "Docker scaffold generator for Laravel projects with dev/prod environments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tayyab",
            "email": "tayyab@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Microsomes\\LaravelDevops\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Microsomes\\LaravelDevops\\LaravelDevopsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
